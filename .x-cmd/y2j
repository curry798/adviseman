
xrc advise
f(){
    local dst; local line; local filename
    cd $(x wsroot)
    find src -name '*.yml' | while read -r line; do
        dst="dst${line#src}"
        dst="${dst%.yml}.t.jso"
        x ensurefp "$dst"
        x yml y2j "$line" | ___x_cmd_advise_gen_advise__merge_awk > "$dst"
    done
}

f