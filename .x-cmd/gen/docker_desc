# shellcheck shell=bash #xsh
_get_advise_json(){
    IFS=" "
    echo "$($* --help 2>/dev/null)"| awk -v indent="$indent" -v CUR_CMD="$*" -f "./dev/awk/get_docker_desc.awk"
}
get(){
    code=$(_get_advise_json "$@" 2>/dev/null)
    # printf "%s" "$code" >&2
    eval "$code"
}
